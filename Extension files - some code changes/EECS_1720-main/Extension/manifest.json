{
  "manifest_version": 2,

  "name": "EECS 1720 Extension Demo",
  "version": "1.0",
  "description": "Can change some settings in Chrome by using this extension.",
  "permissions": ["activeTab"],
  
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
